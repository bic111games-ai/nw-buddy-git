#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/dice-coefficient@2.1.1/node_modules/dice-coefficient/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/dice-coefficient@2.1.1/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/dice-coefficient@2.1.1/node_modules/dice-coefficient/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/dice-coefficient@2.1.1/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dice-coefficient/cli.js" "$@"
else
  exec node  "$basedir/../dice-coefficient/cli.js" "$@"
fi
