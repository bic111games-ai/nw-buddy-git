#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules/@capacitor/cli/bin/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules/@capacitor/cli/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules/@capacitor/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules/@capacitor/cli/bin/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules/@capacitor/cli/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules/@capacitor/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/@capacitor+cli@6.1.2/node_modules:/mnt/d/NW_Faktoria_App4/nw-buddy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@capacitor/cli/bin/capacitor" "$@"
else
  exec node  "$basedir/../@capacitor/cli/bin/capacitor" "$@"
fi
