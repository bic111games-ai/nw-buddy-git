{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "../",
    "baseUrl": "../",
    "moduleResolution": "node",
    "importHelpers": true,
    "module": "CommonJS",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "declaration": false,
    "skipLibCheck": true,
    "allowJs": true
  },
  "buildOptions": {
    "incremental": true,
  },
  "exclude": ["dist"]
}
